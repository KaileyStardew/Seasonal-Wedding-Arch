{
    "Format": "2.9.0",
	
	"ConfigSchema": {
		//Flower Selection Section
		"Spring Arch Type" : {
			"AllowValues": "Tulip, Daffodil, RSV-RidgeAzorean, Wildflour-CreepingDaisy, Wildflour-Mysteria, Cornucopia-Lilac, Cornucopia-MorningGlory, Cornucopia-Rose, Cornucopia-RosePrismatic",
            "Default": "Tulip",
			"Section": "Arch Options"
		},
		"Summer Arch Type" : {
			"AllowValues": "Spangle, SweetPea, Sunflower, RSV-MountainHokkaido, Wildflour-ForestChamomile, Wildflour-Valerian, Cornucopia-Lilac, Cornucopia-Hydrangea, Cornucopia-Lily, Cornucopia-Rose, Cornucopia-RosePrismatic",
            "Default": "Spangle",
			"Section": "Arch Options"
		},
		"Fall Arch Type" : {
			"AllowValues": "Sunflower, FairyRose, RSV-SummitSnowbell, Wildflour-PieBlossom, Wildflour-CaramelBlossom, Cornucopia-Clematis, Cornucopia-Dahlia, Cornucopia-Rose, Cornucopia-RosePrismatic",
            "Default": "FairyRose",
			"Section": "Arch Options"
		},
		"Winter Arch Type" : {
			"AllowValues": "Crocus, Holly, RSV-AuroreanIris, Wildflour-QuartzPrimrose, Wildflour-SugarBlossom, Cornucopia-Rose, Cornucopia-RosePrismatic",
            "Default": "Crocus",
			"Section": "Arch Options"
		},
		"Wood Colour" : {
			"AllowValues": "Default, Birch, Walnut, Mahogany, Oak, Dark, Cornucopia-SpringTrellis, Cornucopia-SummerTrellis, Cornucopia-FallTrellis, Cornucopia-WinterTrellis, Cornucopia-SeasonalTrellis",
            "Default": "Default",
			"Section": "Arch Options"
		},
		//Compatibility Section
		"RSV Arch" : {
			"AllowValues": "true, false",
            "Default": "true",
			"Description": "Edit the arch shown during the Ridgeside Village hotel wedding reception.",
			"Section": "Compatibility"
		},	
		"Custom Wedding Ceremony Decor" : {
			"AllowValues": "true, false",
            "Default": "true",
			"Description": "Replace the misc wedding decorations shown in the custom wedding ceremony to better match the seasonal wedding arch.",
			"Section": "Compatibility"
		},
		"Custom Wedding Ceremony Rug Colour" : {
			"AllowValues": "Green, Beige, White, Original",
            "Default": "Green",
			"Description": "Recolour the decorative rug shown in the custom wedding ceremony to better match the seasonal wedding arch.",
			"Section": "Compatibility"
		},
		"Life Cycle Arch" : {
			"AllowValues": "true, false",
            "Default": "false",
			"Description": "Edit the arch shown during the Rival Heart Events NPC wedding ceremonies. Note: Lewis' positioning in the ceremony means he clips the arch slightly.",
			"Section": "Compatibility"
		},	
		"Enable Mod" : {
			"AllowValues": "true, false",
            "Default": "true",
			"Description": "Toggle this mod without uninstalling it, in case you want to view an event with the original arch. Mostly for debugging.",
			"Section": "Compatibility"
		},	
	},
